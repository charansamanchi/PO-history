{
    "_version": "1.42.0",
    "sap.app": {
        "id": "managedtest1",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.7.5",
            "toolsId": "24e94626-2554-425f-a808-42738835d291"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZARIBA_GR_INVOICE_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "service2": {
                "uri": "/sap/bc/srt/wsdl/flv_10002P111AR1/sdef_url/ZARB_PO_HISTORY/",
                "type": "http",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "currentuser1" : {
                "uri": "/user-api/currentUser",
                "type": "JSON"
          },
          "currentuser2" : {
            "uri": "/user-api/attributes",
            "type": "JSON"
      }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.102.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "managedtest1.i18n.i18n"
                }
            },
            "POheader" : {
                "type" : "sap.ui.model.json.JSONModel"
            },
            "GR" : {
                "type": "sap.ui.model.json.JSONModel"
            },
            "Invoice" : {
                "type": "sap.ui.model.json.JSONModel"
            },
            "POhistory": {
                "dataSource": "mainService",
                "preload": true, 
                "settings": {
                  "defaultBindingMode": "TwoWay",
                  "defaultCountMode": "Inline"
                }
            },
            "userapi" : {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "currentuser1", 
                "preload": true,
                "settings": {
                  "synchronizationMode": "None",
                  "operationMode": "Server",
                  "autoExpandSelect": true,
                  "earlyRequests": true,
                  "groupId": "$direct"
                } 
            },
            "attributes" : {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "currentuser2", 
                "preload": true,
                "settings": {
                  "synchronizationMode": "None",
                  "operationMode": "Server",
                  "autoExpandSelect": true,
                  "earlyRequests": true,
                  "groupId": "$direct"
                } 
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "managedtest1.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteView",
                    "pattern": ":parameterID:",
                    "target": [
                        "TargetView1"
                    ]
                }
            ],
            "targets": {
                "TargetView1": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "View1",
                    "viewName": "View1"
                }
            }
        },
        "rootView": {
            "viewName": "managedtest1.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "managedtest"
    }
}